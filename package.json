{
  "name": "telegram-alert",  // 프로젝트의 이름입니다.
  "version": "1.0.0",  // 프로젝트의 현재 버전입니다.
  "description": "",  // 프로젝트에 대한 간단한 설명을 추가합니다.
  "main": "index.js",  // 프로젝트의 메인 진입 파일입니다.
  "type": "module",  // ECMAScript 모듈을 사용함을 명시합니다.
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",  // 테스트 스크립트로, 테스트가 지정되지 않았을 때 에러 메시지를 출력합니다.
    "start": "node dist/bundle.js",  // 프로덕션 환경에서 애플리케이션을 시작하는 스크립트입니다.
    "build": "WEBPACK_BUILD=true node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --mode production",  // Webpack을 사용하여 프로덕션 모드로 빌드하는 스크립트입니다.
    "dev": "nodemon"  // 개발 모드에서 애플리케이션을 실행하는 스크립트로, nodemon을 사용하여 파일 변경 시 자동으로 재시작합니다.
  },
  "keywords": [],  // 프로젝트와 관련된 키워드를 배열 형태로 추가합니다.
  "author": "",  // 프로젝트의 작성자 정보를 추가합니다.
  "license": "ISC",  // 프로젝트의 라이선스 유형입니다.
  "dependencies": {
    "axios": "^1.6.8",  // HTTP 요청을 처리하기 위한 라이브러리입니다.
    "dotenv": "^16.4.5",  // 환경 변수를 .env 파일에서 로드하기 위한 모듈입니다.
    "express": "^4.19.2",  // Node.js의 웹 애플리케이션 프레임워크입니다.
    "node-telegram-api": "^0.4.0",  // Telegram API를 사용하기 위한 라이브러리입니다 (이 라이브러리는 실제 존재하지 않을 수 있습니다, 확인 필요).
    "node-telegram-bot-api": "^0.65.1",  // Telegram 봇을 만들기 위한 라이브러리입니다.
    "webpack": "^5.91.0",  // 모듈 번들러로, 여러 파일을 하나로 묶어주는 도구입니다.
    "webpack-cli": "^5.1.4"  // Webpack을 커맨드 라인에서 실행할 수 있게 해주는 도구입니다.
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",  // Babel 컴파일러 코어입니다.
    "@babel/preset-env": "^7.24.4",  // 최신 JavaScript를 변환할 수 있게 해주는 Babel 프리셋입니다.
    "@babel/preset-typescript": "^7.24.1",  // TypeScript를 JavaScript로 변환하는 Babel 프리셋입니다.
    "@types/express": "^4.17.21",  // Express의 TypeScript 타입 정의입니다.
    "@types/node": "^20.12.4",  // Node.js의 TypeScript 타입 정의입니다.
    "@types/node-telegram-bot-api": "^0.64.6",  // node-telegram-bot-api 라이브러리의 TypeScript 타입 정의입니다.
    "@types/nodemon": "^1.19.6",  // Nodemon의 TypeScript 타입 정의입니다.
    "@typescript-eslint/eslint-plugin": "^7.5.0",  // TypeScript 코드를 위한 ESLint 플러그인입니다.
    "@typescript-eslint/parser": "^7.5.0",  // ESLint를 사용하여 TypeScript 코드를 파싱하는 패키지입니다.
    "babel-loader": "^9.1.3",  // Webpack에서 Babel을 사용하기 위한 로더입니다.
    "eslint-config-airbnb-base": "^15.0.0",  // Airbnb의 ESLint 설정을 기반으로 한 기본 구성입니다.
    "eslint-config-prettier": "^9.1.0",  // ESLint와 Prettier 간 충돌을 방지하는 구성입니다.
    "eslint-plugin-import": "^2.29.1",  // import/export 구문을 위한 ESLint 플러그인입니다.
    "eslint-plugin-prettier": "^5.1.3",  // Prettier를 ESLint 규칙으로 실행하는 플러그인입니다.
    "nodemon": "^3.1.0",  // 파일 변경 시 자동으로 서버를 재시작할 수 있게 해주는 도구입니다.
    "ts-node": "^10.9.2",  // TypeScript를 직접 실행할 수 있게 해주는 도구입니다.
    "tsconfig-paths": "^4.2.0",  // tsconfig.json 파일에 설정된 경로 별칭을 해석할 수 있게 해주는 패키지입니다.
    "typescript": "^5.4.4"  // TypeScript 언어의 컴파일러입니다.
  }
}
